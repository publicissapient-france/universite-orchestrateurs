{
  "id": "mesos-ui",
  "instances": 1,
  "cpus": 0.2,
  "mem": 512,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "capgemini/mesos-ui:standalone-latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 8500,
          "name": "webui"
        }
      ]
    }
  },
  "env": {
    "ZOOKEEPER_ADDRESS": "mesos-master1.private:2181,mesos-master2.private:2181,mesos-master3.private:2181"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "path": "/",
      "portIndex": 0,
      "protocol": "MESOS_HTTP",
      "timeoutSeconds": 20
    }
  ],
  "labels": {
    "HAPROXY_0_VHOST": "ui.mesos.uo.techx.fr"
  }
}
