{
  "id": "/clickcount-pod",
  "labels": {
    "owner": "ylorenzati"
  },
  "networks": [
    {
      "mode": "container",
      "name": "my-virtual-network-name"
    }
  ],
  "scaling": {
    "kind": "fixed",
    "instances": 1
  },
  "containers": [
    {
      "name": "web",
      "resources": {
        "cpus": 1,
        "mem": 1280
      },
      "endpoints": [
        {
          "name": "http-endpoint",
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": [
            "http"
          ]
        }
      ],
      "image": {
        "id": "xebiafrance/uo-click-count",
        "kind": "DOCKER",
        "forcePull": false
      },
      "user": "root",
      "labels": {
        "owner": "zeus"
      }
    }
  ]
}
